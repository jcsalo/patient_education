	pages = {1363--1371},
	file = {Simon et al_2020_Frailty in older patients undergoing emergency colorectal surgery.pdf:/Users/jcsal/Zotero/storage/4XJLKFJR/Simon et al_2020_Frailty in older patients undergoing emergency colorectal surgery.pdf:application/pdf},
