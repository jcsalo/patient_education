	author = {Huang, Yung-Cheng and Li, Shau-Hsuan and Lu, Hung-I. and Hsu, Chien-Chin and Wang, Yu-Ming and Lin, Wei-Che and Chen, Chao-Jung and Ho, Kuo-Wei and Chiu, Nan-Tsing},
	year = {2019},
	pmid = {30615636},
