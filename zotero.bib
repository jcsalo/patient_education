	number = {2},
	journal = {Annals of Surgery},
	author = {Malietzis, George and Johns, Neil and Al-Hassi, Hafid O. and Knight, Stella C. and Kennedy, Robin H. and Fearon, Kenneth C. H. and Aziz, Omer and Jenkins, John T.},
	month = feb,
