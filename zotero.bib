	keywords = {Humans, Female, Male, Esophageal Neoplasms, Esophagectomy, Esophagus, Stomach, Anastomosis, Surgical, Esophageal Stenosis, Dilatation, Esophageal pH Monitoring, Proton Pump Inhibitors, Surgical Stapling},
