	pmid = {31605347},
	keywords = {Humans, Aged, Female, Gastrointestinal Neoplasms, Male, Middle Aged, Follow-Up Studies, Retrospective Studies, Survival Rate, Prognosis, Aged, 80 and over, Adult, Young Adult, Adolescent, Healthcare Disparities, Race, Socioeconomic Factors, Social Class, SEER Program},
