	number = {1},
	journal = {The Journal of Thoracic and Cardiovascular Surgery},
	author = {van der Schaaf, Maartje K. and Tilanus, Hugo W. and van Lanschot, Jan J. B. and Johar, Asif M. and Lagergren, Pernilla and Lagergren, Jesper and Wijnhoven, Bas P. L.},
