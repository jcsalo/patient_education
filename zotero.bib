	number = {11},
	journal = {European Radiology},
	author = {Nattenmueller, Johanna and Hoegenauer, Hanna and Boehm, Juergen and Scherer, Dominique and Paskow, Michael and Gigic, Biljana and Schrotz-King, Petra and Grenacher, Lars and Ulrich, Cornelia and Kauczor, Hans-Ulrich},
