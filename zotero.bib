	journal = {The Journal of Surgical Research},
	author = {Castillo-Angeles, Manuel and Uyeda, Jennifer W. and Seshadri, Anupamaa J. and Ramsis, Ramsis and Okafor, Barbara U. and Nitzschke, Stephanie and Rangel, Erika L. and Saillant, Noelle N. and Salim, Ali and Askari, Reza},
