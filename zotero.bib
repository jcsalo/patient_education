	number = {2},
	journal = {Annals of the Royal College of Surgeons of England},
	author = {Schizas, D. and Frountzas, M. and Lidoriki, I. and Spartalis, E. and Toutouzas, K. and Dimitroulis, D. and Liakakos, T. and Mylonas, K. S.},
