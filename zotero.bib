	author = {Futier, Emmanuel and Constantin, Jean-Michel and Paugam-Burtz, Catherine and Pascal, Julien and Eurin, Mathilde and Neuschwander, Arthur and Marret, Emmanuel and Beaussier, Marc and Gutton, Christophe and Lefrant, Jean-Yves and Allaouchiche, Bernard and Verzilli, Daniel and Leone, Marc and De Jong, Audrey and Bazin, Jean-Etienne and Pereira, Bruno and Jaber, Samir and {IMPROVE Study Group}},
	month = aug,
