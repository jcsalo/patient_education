	keywords = {Humans, Magnetic Resonance Imaging, Tomography, X-Ray Computed, Absorptiometry, Photon, Intra-Abdominal Fat, Methods, ICC, Obesity, Abdominal, Plethysmography},
	pages = {1--10},
