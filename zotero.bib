	keywords = {Humans, Length of Stay, Laparoscopy, Postoperative Complications, Reoperation, Recurrence, Quality of Life, Operative Time, Robotic Surgical Procedures, Herniorrhaphy, Hospital Costs, Emergency Service, Hospital, Hernia, Ventral},
