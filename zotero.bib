	number = {5},
	journal = {CMAJ: Canadian Medical Association journal = journal de l'Association medicale canadienne},
	author = {Rockwood, Kenneth and Song, Xiaowei and MacKnight, Chris and Bergman, Howard and Hogan, David B. and McDowell, Ian and Mitnitski, Arnold},
	month = aug,
