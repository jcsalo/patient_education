	pmcid = {PMC5658455},
	keywords = {Humans, Muscle, Skeletal, Female, Male, Middle Aged, Follow-Up Studies, Retrospective Studies, Survival Rate, Prognosis, Adenocarcinoma, Carcinoma, Squamous Cell, Esophageal Neoplasms, Esophagectomy, Combined Modality Therapy, Thoracic Neoplasms},
