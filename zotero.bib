	keywords = {Body Composition, Humans, Tomography, X-Ray Computed, Aged, Female, Male, Middle Aged, Obesity, Sarcopenia, Treatment Outcome, Body Weight, Pancreatic Neoplasms, Prognosis, Overweight, Risk, Pancreatic},
	pages = {6973--6979},
