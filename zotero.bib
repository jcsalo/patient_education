	number = {4},
	journal = {Journal of Cachexia, Sarcopenia and Muscle},
	author = {Fogelman, David R. and Holmes, Holly and Mohammed, Khalil and Katz, Matthew H. G. and Prado, Carla M. and Lieffers, Jessica and Garg, Naveen and Varadhachary, Gauri R. and Shroff, Rachna and Overman, Michael J. and Garrett, Christopher and Wolff, Robert A. and Javle, Milind},
