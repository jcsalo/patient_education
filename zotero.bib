	number = {11-12},
	journal = {Nutrition (Burbank, Los Angeles County, Calif.)},
	author = {Sandini, Marta and Bernasconi, Davide P. and Fior, Davide and Molinelli, Matilde and Ippolito, Davide and Nespoli, Luca and Caccialanza, Riccardo and Gianotti, Luca},
