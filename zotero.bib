	number = {30},
	journal = {Journal of Clinical Oncology: Official Journal of the American Society of Clinical Oncology},
	author = {Allum, William H. and Stenning, Sally P. and Bancewicz, John and Clark, Peter I. and Langley, Ruth E.},
	month = oct,
