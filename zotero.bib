	pages = {1023--1029},
	file = {Mariette et al_2020_Health-related Quality of Life Following Hybrid Minimally Invasive Versus Open.pdf:/Users/jcsal/Zotero/storage/LF554P7Z/Mariette et al_2020_Health-related Quality of Life Following Hybrid Minimally Invasive Versus Open.pdf:application/pdf},
