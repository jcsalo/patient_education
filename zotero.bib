	journal = {Journal of Clinical Oncology: Official Journal of the American Society of Clinical Oncology},
	author = {Markar, Sheraz R. and Ni, Melody and Gisbertz, Suzanne S. and van der Werf, Leonie and Straatman, Jennifer and van der Peet, Donald and Cuesta, Miguel A. and Hanna, George B. and van Berge Henegouwen, Mark I. and {Dutch Upper GI Cancer Audit and TIME Study Group}},
