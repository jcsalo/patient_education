	keywords = {Body Composition, Humans, Aged, Female, Gastrointestinal Neoplasms, Hospitalization, Length of Stay, Male, Middle Aged, Neoadjuvant Therapy, Obesity, Pilot Projects, Postoperative Care, Prevalence, Sarcopenia, Treatment Outcome},
