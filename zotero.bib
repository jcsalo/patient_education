	author = {Ruppert, R. and Junginger, T. and Ptok, H. and Strassburg, J. and Maurer, C. A. and Brosi, P. and Sauer, J. and Baral, J. and Kreis, M. and Wollschlaeger, D. and Hermanek, P. and Merkel, S. and {OCUM group}},
	month = oct,
