	journal = {Endoscopy},
	author = {Ellrichmann, M. and Sergeev, P. and Bethge, J. and Arlt, A. and Topalidis, T. and Ambrosch, P. and Wiltfang, J. and Fritscher-Ravens, A.},
	month = jul,
