	pmid = {15051778},
	keywords = {Humans, Aged, Female, Male, Middle Aged, Prospective Studies, Neoplasms, Antineoplastic Agents, Quality of Life, Administration, Oral, Cancer, Infusions, Intravenous, Anemia, Iron, Erythropoietin, Recombinant Proteins, Abnormalities, Drug-Induced, Iron-Dextran Complex},
