	pmid = {30395362},
	keywords = {Humans, Aged, Female, Male, Middle Aged, Treatment Outcome, Survival Rate, Cohort Studies, Adult, Esophageal Neoplasms, Esophagectomy, Chemoradiotherapy, Esophageal Squamous Cell Carcinoma, Propensity Score, Taiwan, dCRT, Tri},
