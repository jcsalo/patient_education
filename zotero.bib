	keywords = {Humans, Muscle, Skeletal, Tomography, X-Ray Computed, Female, Male, Middle Aged, Sarcopenia, Colorectal Neoplasms, Prognosis, Adult, sarcopenia, Survival Analysis, myosteatosis, colorectal cancer, skeletal muscle gauge, skeletal muscle index, skeletal muscle radiodensity},
	pages = {8451--8461},
