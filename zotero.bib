	file = {Decoster et al_2015_Screening tools for multidimensional health problems warranting a geriatric.pdf:/Users/jcsal/Zotero/storage/CCTMH6WF/Decoster et al_2015_Screening tools for multidimensional health problems warranting a geriatric.pdf:application/pdf},
