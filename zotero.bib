	keywords = {Humans, Female, Male, Middle Aged, Neoadjuvant Therapy, Survival Rate, Prognosis, Chemotherapy, Adjuvant, Disease-Free Survival, Esophageal Neoplasms, Esophagectomy, Combined Modality Therapy, Lymphatic Metastasis, Radiotherapy, Adjuvant, Carcinoma, naCRT},
