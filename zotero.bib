	keywords = {Humans, Sarcopenia, Prognosis, Meta-analysis, Esophageal Neoplasms, Esophagectomy, Oesophageal cancer, Postoperative Complications, Anastomotic Leak, Postoperative complications},
	pages = {120--132},
	file = {Schizas et al_2020_Sarcopenia does not affect postoperative complication rates in oesophageal.pdf:/Users/jcsal/Zotero/storage/XYWMIZC7/Schizas et al_2020_Sarcopenia does not affect postoperative complication rates in oesophageal.pdf:application/pdf},
