	number = {6},
	journal = {Journal of Cachexia, Sarcopenia and Muscle},
	author = {Ebadi, Maryam and Wang, Connie W. and Lai, Jennifer C. and Dasarathy, Srinivasan and Kappus, Matthew R. and Dunn, Michael A. and Carey, Elizabeth J. and Montano-Loza, Aldo J. and {From the Fitness, Life Enhancement, and Exercise in Liver Transplantation (FLEXIT) Consortium}},
