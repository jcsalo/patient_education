	keywords = {Humans, Muscle, Skeletal, Tomography, X-Ray Computed, Pilot Projects, Sarcopenia, Adult, body composition, United States, computed tomography, skeletal muscle, National Institutes of Health (U.S.), NIH ImageJ},
