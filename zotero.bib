	pmid = {32500418},
	keywords = {Humans, Databases, Factual, United States, Esophageal Neoplasms, Esophagectomy, Esophageal cancer, Healthcare Disparities, Race, African Americans, Whites, NCDB, Racial disparities},
	pages = {581--592},
}

@article{paniaguacruze498,
	title = {Association {Between} {Marital} {Status} and {Racial} {Disparities} in {Esophageal} {Cancer} {Care}},
