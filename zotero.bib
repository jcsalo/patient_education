	number = {6},
	journal = {Annals of Gastroenterological Surgery},
	author = {Morimoto, Yoshihiro and Takahashi, Hidekazu and Fujii, Makoto and Miyoshi, Norikatsu and Uemura, Mamoru and Matsuda, Chu and Yamamoto, Hirofumi and Mizushima, Tsunekazu and Mori, Masaki and Doki, Yuichiro},
