	pmid = {22585691},
	pmcid = {PMC4517071},
	keywords = {Humans, Female, Male, Treatment Outcome, Antineoplastic Combined Chemotherapy Protocols, Fluorouracil, Leucovorin, Survival Analysis, Stomach Neoplasms, Adenocarcinoma, Chemoradiotherapy, Adjuvant, Gastrectomy},
	pages = {2327--2333},
