	keywords = {Humans, Aged, Female, Length of Stay, Male, Middle Aged, Treatment Outcome, Retrospective Studies, Risk Factors, Risk Assessment, Prognosis, Adult, Predictive Value of Tests, Databases, Factual, Postoperative Complications, Multivariate Analysis, Reoperation, Digestive System Surgical Procedures, Rectum, Morbidity, Health Status Indicators, outcomes, Colon, Frailty, Patient Discharge, Postoperative Period, NSQIP, mFrailty5, colorectal, postoperative},
