	file = {Balch et al_2023_Methods and evaluation metrics for reducing material waste in the operating room.pdf:/Users/jcsal/Zotero/storage/L88AIB7Q/Balch et al_2023_Methods and evaluation metrics for reducing material waste in the operating room.pdf:application/pdf},
