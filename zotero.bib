	keywords = {Humans, Aged, Female, Male, Middle Aged, Postoperative Care, Treatment Outcome, Colorectal Neoplasms, Exercise Therapy, Preoperative Care, Exercise, Prehab, Walking, Exercise Tolerance, Nutrition Therapy, Resistance Training, Single-Blind Method},
