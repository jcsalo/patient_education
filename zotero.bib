	pmid = {11870157},
	keywords = {Humans, Aged, Female, Male, Middle Aged, Treatment Outcome, Aged, 80 and over, Adult, Adenocarcinoma, Carcinoma, Squamous Cell, Esophageal Neoplasms, Neoplasm Recurrence, Local, Combined Modality Therapy, Radiotherapy Dosage, Radiotherapy},
