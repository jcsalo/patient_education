	keywords = {Humans, Mortality, Health Services Accessibility, Socioeconomic Factors, Vulnerable Populations, Diabetes Mellitus, Primary Health Care, SDI, Infant Mortality, Infant, Low Birth Weight, Infant, Newborn, Needs Assessment},
	pages = {539--559},
