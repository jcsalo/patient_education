	keywords = {Humans, Treatment Outcome, Randomized Controlled Trials as Topic, Survival Analysis, Surgery, Perioperative Care, mortality, perioperative care, Critical Care, Blood Transfusion, Critical Illness, anesthesia, blood transfusion, critical illness},
	pages = {511--519},
