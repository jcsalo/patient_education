	keywords = {Humans, Treatment Outcome, Retrospective Studies, Prospective Studies, Cohort Studies, Esophageal Neoplasms, Esophagectomy, Postoperative Complications, Anastomotic Leak, Minimally Invasive Surgical Procedures, Pneumonia},
	pages = {919--923},
