	keywords = {Humans, Female, Male, Middle Aged, Treatment Outcome, Retrospective Studies, Radiography, Esophagectomy, Patient Selection, Preoperative Care, Anastomosis, Surgical, Colon, Mass Screening, Mesenteric Arteries},
