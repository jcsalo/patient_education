	keywords = {Humans, Aged, Female, Male, Follow-Up Studies, Neoplasms, Aged, 80 and over, Comorbidity, Registries, Mortality, North Carolina, Frailty, Geriatric Assessment, Frail Elderly, cFI},
	pages = {894--902},
	file = {Guerard et al_2017_Frailty Index Developed From a Cancer-Specific Geriatric Assessment and the.pdf:/Users/jcsal/Zotero/storage/QMMXDLY4/Guerard et al_2017_Frailty Index Developed From a Cancer-Specific Geriatric Assessment and the.pdf:application/pdf},
