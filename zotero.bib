	file = {Kim et al_2021_Clinical significance of skeletal muscle density and sarcopenia in patients.pdf:/Users/jcsal/Zotero/storage/5AB24AE6/Kim et al_2021_Clinical significance of skeletal muscle density and sarcopenia in patients.pdf:application/pdf},
