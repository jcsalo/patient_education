	keywords = {Humans, Aged, Female, Male, Middle Aged, Neoadjuvant Therapy, Antineoplastic Combined Chemotherapy Protocols, Adult, Fluorouracil, Leucovorin, Survival Analysis, Carcinoma, Squamous Cell, Cisplatin, Esophageal Neoplasms, Combined Modality Therapy, ChemoRT\_Surgery, Etoposide},
