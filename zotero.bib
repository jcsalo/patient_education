	note = {tex.ids= minnella1081a},
	keywords = {Humans, Aged, Female, Male, Middle Aged, Stomach Neoplasms, Esophageal Neoplasms, Nutritional Status, Preoperative Care, Exercise, Prehabilitation, Walking},
	pages = {1081--1089},
	file = {Minnella et al_2018_Effect of Exercise and Nutrition Prehabilitation on Functional Capacity in.pdf:/Users/jcsal/Zotero/storage/H8ZDI237/Minnella et al_2018_Effect of Exercise and Nutrition Prehabilitation on Functional Capacity in.pdf:application/pdf},
}

