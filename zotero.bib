	keywords = {Humans, Aged, Female, Gastrointestinal Neoplasms, Male, Neoadjuvant Therapy, Follow-Up Studies, Neoplasm Staging, Risk Factors, Survival Rate, Prognosis, Age Factors, Chemotherapy, Adjuvant, United States, Combined Modality Therapy, Comorbidity, Netherlands, Radiotherapy, Adjuvant, Heart Diseases, Lung Diseases, Postoperative Period, Diabetes Mellitus, Vascular Diseases},
