	pmid = {18487570},
	keywords = {Humans, Aged, Female, Male, Middle Aged, Neoplasm Staging, Lung Neoplasms, Antineoplastic Agents, CRC, Aged, 80 and over, Adult, Age Factors, Chemotherapy, Adjuvant, Colonic Neoplasms, Comorbidity, Decision Making, Physicians},
