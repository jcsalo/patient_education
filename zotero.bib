	author = {Chiu, Philip W. Y. and Chan, Angus C. W. and Leung, S. F. and Leong, H. T. and Kwong, K. H. and Li, Micheal K. W. and Au-Yeung, Alex C. M. and Chung, Sydney C. S. and Ng, Enders K. W.},
	month = aug,
