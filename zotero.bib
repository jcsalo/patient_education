	keywords = {Body Composition, Humans, Muscle, Skeletal, Aged, Female, Gastrointestinal Neoplasms, Male, Middle Aged, Obesity, Body Mass Index, Follow-Up Studies, Neoplasm Staging, Risk Factors, Cachexia, Lung Neoplasms, Survival Rate, Prognosis, Adult, Age Factors, Canada, Cancer Care Facilities},
	pages = {1539--1547},
	file = {Martin et al_2013_Cancer cachexia in the age of obesity.pdf:/Users/jcsal/Zotero/storage/3UK457E7/Martin et al_2013_Cancer cachexia in the age of obesity.pdf:application/pdf},
