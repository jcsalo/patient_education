	pmid = {31665455},
	pmcid = {PMC7105351},
	keywords = {Adipose Tissue, Body Composition, Humans, Aged, Female, Male, Middle Aged, Obesity, Adult, Young Adult, Adolescent, Glucagon-Like Peptide 1, GLP-1, appetite, Appetite, body fat, Double-Blind Method, Food Preferences, Gastrointestinal Hormones, GLP-1 analog, Hypoglycemic Agents, liraglutide, Liraglutide, obesity, Placebos, Satiation, Taste, taste preference},
