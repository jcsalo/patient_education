	keywords = {Humans, Tomography, X-Ray Computed, Aged, Female, Male, Middle Aged, Colorectal Neoplasms, Risk, Aged, 80 and over, Age Factors, Screening, Early Detection of Cancer, Colonoscopy, Occult Blood, Sigmoidoscopy},
